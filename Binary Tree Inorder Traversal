class Solution {
    private:
    void Solve(TreeNode* root,vector<int> &vec){
        if(root==NULL){
            return;
        }

        Solve(root->left,vec);

        vec.push_back(root->val);

        Solve(root->right,vec);
    }
public:
    vector<int> inorderTraversal(TreeNode* root) {
         vector<int> vec;
         Solve(root,vec);
         return vec;
    }
};
